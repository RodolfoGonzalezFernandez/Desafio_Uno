{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "RestApisUrls": {
    "Collection": [
      {
        "Name": "GDD",
        "Endpoint": "https://periodos-perdidos-previred.azurewebsites.net/",
        "TimeoutInSeconds": 10,
        "Accept": "application/json",
        "Token": "",
        "Headers": [],
        "TransientHttpErrors": {
          "WaitAndRetryClassic": {
            "Enable": true,
            "TotalRetry": 3,
            "WaitMilliseconds": 1000
          },
          "WaitAndRetryExponential": {
            "Enable": false,
            "TotalRetry": 3,
            "WaitMilliseconds": 1000
          },
          "CircuitBreaker": {
            "Enable": true,
            "MaximumFails": 10,
            "DurationInSeconds": 30
          }
        }
      }
    ]
  }
}
